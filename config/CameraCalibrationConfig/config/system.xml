<?xml version="1.0" encoding="iso-8859-1" standalone="no"?>
<adtf:project xmlns:adtf="adtf">
 <general_settings>
  <property name="active_configuration" type="String" value="Camera_Calibration" />
  <property name="media_description_files" type="String" value="./../description/*.description;./../addons/*/description/*.description;./../src/examples/src/description/*.description;../../../../description/*.description/*.*;../../../description/aadc.description" />
  <property name="media_description_enable_provide_signals" type="Bool" value="true" />
  <property name="enable_messagebus" type="Bool" value="true" />
 </general_settings>
 <configurations>
  <configuration description="" name="Camera_Calibration">
   <graph>
    <filter alias="Video_Display" idref="adtf.visual.video_display" priority="1">
     <settings>
      <property name="active" type="Bool" value="true" />
     </settings>
    </filter>
    <filter alias="Signal_View" idref="adtf.ui.signal_view" priority="1" />
    <connection connection_priority="50" dest="Video_Display.input" interface="" source="AADC_Marker_Detection_Filter.Video_RGB_output" />
    <filter alias="AADC_Marker_Detection_Filter" idref="adtf.aadc.aadc_markerDetection" priority="2">
     <settings>
      <property name="Calibration File for used Camera" type="String" value="..\..\..\configuration_files\xtionIntrinsicCalib.yml" />
      <property name="Dictionary File For Markers" type="String" value="..\..\..\configuration_files\roadsign.yml" />
     </settings>
    </filter>
    <filter alias="AADC_Camera_Calibration" idref="adtf.aadc.aadc_cameraCalibration" priority="3">
     <settings />
    </filter>
    <filter alias="AADC_Marker_Evaluator" idref="adtf.aadc.aadc_markerEvaluator" priority="4">
     <settings />
    </filter>
    <connection connection_priority="50" dest="AADC_Marker_Evaluator.Road_Sign" interface="" source="AADC_Marker_Detection_Filter.RoadSign" />
    <filter alias="AADC_Xtion_Camera" idref="adtf.aadc.aadc_xtionCamera" priority="5">
     <settings>
      <property name="Configuration File" type="String" value="..\..\..\configuration_files\xtionSettings.xml" />
     </settings>
    </filter>
    <connection connection_priority="50" dest="AADC_Marker_Detection_Filter.Video_RGB_input" interface="" source="AADC_Xtion_Camera.Video_RGB" />
    <connection connection_priority="50" dest="AADC_Camera_Calibration.Video_RGB_input" interface="" source="AADC_Xtion_Camera.Video_RGB" />
   </graph>
  </configuration>
 </configurations>
 <fileinfo adtfversion="2.13.1" configversion="1.3" />
